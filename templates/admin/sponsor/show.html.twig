{% extends 'admin/base_admin.html.twig' %}

{% block title %}Sponsor{% endblock %}

{% block body %}
    <h1>Sponsor</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sponsor.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ sponsor.nom }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ sponsor.description }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ sponsor.logo }}</td>
            </tr>
            <tr>
                <th>SiteWeb</th>
                <td>{{ sponsor.siteWeb }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ sponsor.type }}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ sponsor.montant }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ sponsor.dateDebut ? sponsor.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateFin</th>
                <td>{{ sponsor.dateFin ? sponsor.dateFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ sponsor.statut }}</td>
            </tr>
            <tr>
                <th>ContactPersonne</th>
                <td>{{ sponsor.contactPersonne }}</td>
            </tr>
            <tr>
                <th>ContactEmail</th>
                <td>{{ sponsor.contactEmail }}</td>
            </tr>
            <tr>
                <th>ContactTelephone</th>
                <td>{{ sponsor.contactTelephone }}</td>
            </tr>
            <tr>
                <th>DateCreation</th>
                <td>{{ sponsor.dateCreation ? sponsor.dateCreation|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_sponsor_index') }}">back to list</a>

    <a href="{{ path('admin_sponsor_edit', {'id': sponsor.id}) }}">edit</a>

    {{ include('admin/sponsor/_delete_form.html.twig') }}
{% endblock %}
